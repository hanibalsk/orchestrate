              "left": "Input Tokens",
              "operator": "*",
              "reducer": "sum",
              "right": "0.000003"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Output Cost",
            "binary": {
              "left": "Output Tokens",
              "operator": "*",
              "reducer": "sum",
              "right": "0.000015"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Total Cost",
            "binary": {
              "left": "Input Cost",
              "operator": "+",
              "reducer": "sum",
              "right": "Output Cost"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        }
